<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:stretchColumns="0">
        <TableRow>
            <TextView
                android:id="@+id/tv1"
                android:layout_gravity="center_horizontal"
                android:text="그림 1"
                android:textSize="15dp"
                ></TextView>
            <RatingBar
                android:id="@+id/rbar1"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"
                ></RatingBar>
        </TableRow>
        <TableRow>
            <TextView
                android:id="@+id/tv2"
                android:layout_gravity="center_horizontal"
                android:text="그림 2"
                android:textSize="15dp"
                ></TextView>
            <RatingBar
                android:id="@+id/rbar2"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"
                ></RatingBar>
        </TableRow>
        <TableRow>
            <TextView
                android:id="@+id/tv3"
                android:layout_gravity="center_horizontal"
                android:text="그림 3"
                android:textSize="15dp"
                ></TextView>
            <RatingBar
                android:id="@+id/rbar3"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"
                ></RatingBar>
        </TableRow>
        <TableRow>
            <TextView
                android:id="@+id/tv4"
                android:layout_gravity="center_horizontal"
                android:text="그림 4"
                android:textSize="15dp"
                ></TextView>
            <RatingBar
                android:id="@+id/rbar4"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"
                ></RatingBar>
        </TableRow>
        <TableRow>
            <TextView
                android:id="@+id/tv5"
                android:layout_gravity="center_horizontal"
                android:text="그림 5"
                android:textSize="15dp"
                ></TextView>
            <RatingBar
                android:id="@+id/rbar5"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"
                ></RatingBar>
        </TableRow>
        <TableRow>
            <TextView
                android:id="@+id/tv6"
                android:layout_gravity="center_horizontal"
                android:text="그림 6"
                android:textSize="15dp"
                ></TextView>
            <RatingBar
                android:id="@+id/rbar6"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"
                ></RatingBar>
        </TableRow>

        <Button
            android:id="@+id/btnReturn"
            android:text="돌아가기"
            ></Button>
    </TableLayout>

</LinearLayout>